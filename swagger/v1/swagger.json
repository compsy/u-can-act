                "first_name": {
                  "type": "string"
                },
                "last_name": {
                  "type": "string"
                },
                "gender": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "mobile_phone": {
                  "type": "string"
                }
              }
            }
          },
