    "/basic_auth_api/person/show_list": {
      "get": {
        "summary": "Shows a list of persons",
        "tags": [
          "Person"
        ],
        "consumes": [
          "application/json"
        ],
        "security": [
          {
            "BasicAuth": {
            }
          }
        ],
        "parameters": [
          {
            "name": "person",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "person_auth0_ids": {
                  "type": "array"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "lists all people"
          },
          "401": {
            "description": "not authenticated"
          }
        }
      }
    },
