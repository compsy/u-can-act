    "/person": {
      "put": {
        "summary": "Updates the current user",
        "tags": [
          "Person"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "person",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "mobile_phone": {
